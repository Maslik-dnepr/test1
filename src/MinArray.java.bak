public class MinArray {

	public static void main(String[] args)
	{
		int[] at={22,55,33,22,8,1,45,7};
		System.out.println(funcMin(at));
		       
	}
	
	static int funcMin(int[] ar)
	{
		int result=ar[0];
		for (int i=1;i<ar.length;i++)
		{
			result=(ar[i]<result)?ar[i]:result;
		}
		return result;
	}

}
